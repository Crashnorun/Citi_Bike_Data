<UserControl x:Class="Citi_Bike_Data_02.UI.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Citi_Bike_Data_02.UI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <ProgressBar Margin="10,0,10,10" Height="10" VerticalAlignment="Bottom" Foreground="{StaticResource CitiBikeColorHEX}"/>
        <Label Content="Label" Margin="10,0,10,20" Height="26" VerticalAlignment="Bottom"/>
        <Button x:Name="btn_CreateDB" Content="CreateDB" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="btn_CreateDB_Click"/>

    </Grid>
</UserControl>
